{
  "expo": {
    "name": "Darawal",
    "slug": "Darawal",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Darawal helps mechanics find nearby jobs and clients. Allow continuous location access for efficient job matching."
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darawal.mechanicc",
      "buildNumber": "4",
      "purposeStrings": {
        "NSLocationAlwaysUsageDescription": "Darawal helps mechanics find nearby jobs and clients. Allow continuous location access for efficient job matching.",
        "NSLocationWhenInUseUsageDescription": "Darawal shows relevant jobs near your location. Allow location access only while actively searching for jobs."
      }
    },
    "android": {
      "package": "com.darawal.mechanic",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA47XXKuNkOS0Z6m0RdyKsRgw9ydRr35ww"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "a81fead0-eeff-4561-b691-eeaf41348613"
      }
    }
  }
}